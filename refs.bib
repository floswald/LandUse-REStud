@article{schaubergeretal2022,
	abstract = {Agricultural performance is influenced by environmental conditions, management decisions and economic circumstances. It is important to quantify their respective contribution to allow for detecting major hazards to production, projecting future yields under climate change and deriving adaptation options. For this purpose, time series of agricultural yields with high spatial and long-term temporal resolution are a primary requisite. Here we present a data set of crop performance in France, one of Europe's major crop producers. The data set comprises ten crops (barley, maize, oats, potatoes, rapeseed, sugarbeet, sunflower, durum wheat, soft wheat and wine) and covers the years 1900 to 2018. It contains harvested area, production and yield data for all 96 French d{\'e}partements (i.e. counties or NUTS3 level) with a total number of 375,264 data points. Entries until 1988 have been digitized manually from statistical yearbooks. The technical validation indicates a high consistency of the data set within itself and with external resources. The data set may contribute to an enhanced understanding of the manifold influences on agricultural performance.},
	author = {Schauberger, Bernhard and Kato, Hiromi and Kato, Tomomichi and Watanabe, Daiki and Ciais, Philippe},
	date = {2022/02/03},
	date-added = {2022-11-15 09:43:46 +0100},
	date-modified = {2022-11-15 09:43:46 +0100},
	doi = {10.1038/s41597-022-01145-4},
	id = {Schauberger2022},
	isbn = {2052-4463},
	journal = {Scientific Data},
	number = {1},
	pages = {38},
	title = {French crop yield, area and production data for ten staple crops from 1900 to 2018 at county resolution},
	url = {https://doi.org/10.1038/s41597-022-01145-4},
	volume = {9},
	year = {2022},
	bdsk-url-1 = {https://doi.org/10.1038/s41597-022-01145-4}}

